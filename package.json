{
  "publisher": "get-snippets",
  "name": "get-snippets",
  "displayName": "GetX Snippets",
  "description": "An extension to accelerate the process of developing applications with flutter, aimed at everyone using the Get package.",
  "version": "2.5.0",
  "engines": {
    "vscode": "^1.2.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:extension.create-new-folder"
  ],
  "main": "./out/extension.js",
  "keywords": [
    "dart",
    "flutter",
    "angulardart",
    "get",
    "state-management"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kauemurakami/get_snippets_extension.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.create-new-folder",
        "title": "Init getx_pattern project"
      }
    ],
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets.code-snippets"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/kauemurakami/get_snippets_extension/issues"
  },
  "directories": {
    "example": "examples"
  },
  "author": "kauetmurakami",
  "license": "ISC",
  "dependencies": {
    "vscode": "^1.1.37",
    "semver": "^6.3.0",
    "vss-web-extension-sdk": "^5.141.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "typescript": "^3.5.1",
    "vscode": "^1.1.33"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  }
}
