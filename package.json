{
  "publisher": "get-snippets",
  "name": "get-snippets",
  "displayName": "Get Snippets",
  "description": "Uma extensão para acelerar o processo de desenvolvimento de aplicações com flutter, destinado a todos que utilizam o package Get.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.2.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "dart",
    "flutter",
    "angulardart",
    "get",
    "state-management"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kauemurakami/get_snippets_extension.git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets.code-snippets"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/kauemurakami/get_snippets_extension/issues"
  },
  "directories": {
    "example": "examples"
  },
  
  "author": "kauetmurakami",
  "license": "ISC",
  "dependencies": {
    "vss-web-extension-sdk": "^5.141.0"
  }
}
